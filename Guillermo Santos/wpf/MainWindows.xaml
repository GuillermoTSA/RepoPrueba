<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1" d:DataContext="{d:DesignInstance Type=local:ConcatenarNombre}"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Height="150" VerticalAlignment="Top" Name="TextBoxNombre" Text="{Binding Path=Nombre, Mode=TwoWay}"></TextBox>
        <TextBox Height="150" VerticalAlignment="Center" Name="TextBoxNombreCompleto" Text="{Binding Path=NombreCompleto, Mode=TwoWay}"></TextBox>
        <TextBox Height="150" VerticalAlignment="Bottom" Name="TextBoxApellido" Text="{Binding Path=Apellido, Mode=TwoWay}"></TextBox>
    </Grid>
</Window>
